<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE psddl SYSTEM "psddl.dtd">

<psddl>

    <use file="psddldata/fli.ddl.xml" />

    <package name="Fli">

        <!-- ========================================================================= -->
    
        <h5schema name="FrameV1" version="0">

            <dataset name="frame">
                <attribute name="shotIdStart"/>
                <attribute name="readoutTime"/>
                <attribute name="temperature"/>
            </dataset>
            
            <dataset name="data">
                <attribute name="data"/>
            </dataset>
            
        </h5schema>
    
    </package>
</psddl>
