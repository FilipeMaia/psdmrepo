<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE psddl SYSTEM "psddl.dtd">

<psddl>

    <use file="psddldata/lusi.ddl.xml" />

    <package name="Lusi">

        <!-- ========================================================================= -->
    
        <h5schema name="DiodeFexConfigV1" version="0">

            <dataset name="config">
                <attribute name="base"/>
                <attribute name="scale"/>
            </dataset>
            
        </h5schema>
    
        <!-- ========================================================================= -->
    
        <h5schema name="DiodeFexConfigV2" version="0">

            <dataset name="config">
                <attribute name="base"/>
                <attribute name="scale"/>
            </dataset>
            
        </h5schema>
    
        <!-- ========================================================================= -->
    
        <h5schema name="IpmFexConfigV1" version="0">

            <dataset name="config">
                <attribute name="diode"/>
                <attribute name="xscale"/>
                <attribute name="yscale"/>
            </dataset>
            
        </h5schema>
    
        <!-- ========================================================================= -->
    
        <h5schema name="IpmFexConfigV2" version="0">

            <dataset name="config">
                <attribute name="diode"/>
                <attribute name="xscale"/>
                <attribute name="yscale"/>
            </dataset>
            
        </h5schema>
    
        <!-- ========================================================================= -->
    
        <h5schema name="IpmFexV1" version="0">

            <dataset name="data">
                <attribute name="channel"/>
                <attribute name="sum"/>
                <attribute name="xpos"/>
                <attribute name="ypos"/>
            </dataset>
            
        </h5schema>
    
    </package>
</psddl>
