<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE psddl SYSTEM "psddl.dtd">

<psddl>

    <use file="psddldata/princeton.ddl.xml" />

    <package name="Princeton">

        <!-- ========================================================================= -->
    
        <h5schema name="FrameV1" version="0">

            <dataset name="data">
                <attribute name="shotIdStart"/>
                <attribute name="readoutTime"/>
            </dataset>
            
            <dataset name="image" method="data"/>
            
        </h5schema>
    
        <!-- ========================================================================= -->
    
        <h5schema name="FrameV2" version="0">

            <dataset name="data">
                <attribute name="shotIdStart"/>
                <attribute name="readoutTime"/>
                <attribute name="temperature"/>
            </dataset>
            
            <dataset name="image" method="data"/>
            
        </h5schema>
    
    </package>
</psddl>
