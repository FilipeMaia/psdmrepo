<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE psddl SYSTEM "psddl.dtd">

<psddl>

    <use file="psddl_hdf2psana/lusi-h5.ddl.xml" cpp_headers="psddl_hdf2psana/lusi.ddl.h"/>
    
    <package name="Bld">

        <!-- ========================================================================= -->
        <h5schema name="BldDataEBeamV0" version="0">
            <dataset name="data">
                <attribute name="uDamageMask" method="damageMask"/>
                <attribute name="fEbeamCharge" method="ebeamCharge"/>
                <attribute name="fEbeamL3Energy" method="ebeamL3Energy"/>
                <attribute name="fEbeamLTUPosX" method="ebeamLTUPosX"/>
                <attribute name="fEbeamLTUPosY" method="ebeamLTUPosY"/>
                <attribute name="fEbeamLTUAngX" method="ebeamLTUAngX"/>
                <attribute name="fEbeamLTUAngY" method="ebeamLTUAngY"/>
            </dataset>
        </h5schema>

        <!-- ========================================================================= -->
        <h5schema name="BldDataEBeamV1" version="0">
            <dataset name="data">
                <attribute name="uDamageMask" method="damageMask"/>
                <attribute name="fEbeamCharge" method="ebeamCharge"/>
                <attribute name="fEbeamL3Energy" method="ebeamL3Energy"/>
                <attribute name="fEbeamLTUPosX" method="ebeamLTUPosX"/>
                <attribute name="fEbeamLTUPosY" method="ebeamLTUPosY"/>
                <attribute name="fEbeamLTUAngX" method="ebeamLTUAngX"/>
                <attribute name="fEbeamLTUAngY" method="ebeamLTUAngY"/>
                <attribute name="fEbeamPkCurrBC2" method="ebeamPkCurrBC2"/>
            </dataset>
        </h5schema>
        <!-- ========================================================================= -->
        <h5schema name="BldDataEBeamV2" version="0">
            <dataset name="data">
                <attribute name="uDamageMask" method="damageMask"/>
                <attribute name="fEbeamCharge" method="ebeamCharge"/>
                <attribute name="fEbeamL3Energy" method="ebeamL3Energy"/>
                <attribute name="fEbeamLTUPosX" method="ebeamLTUPosX"/>
                <attribute name="fEbeamLTUPosY" method="ebeamLTUPosY"/>
                <attribute name="fEbeamLTUAngX" method="ebeamLTUAngX"/>
                <attribute name="fEbeamLTUAngY" method="ebeamLTUAngY"/>
                <attribute name="fEbeamPkCurrBC2" method="ebeamPkCurrBC2"/>
                <attribute name="fEbeamEnergyBC2" method="ebeamEnergyBC2"/>
            </dataset>
        </h5schema>
        <!-- ========================================================================= -->
        <h5schema name="BldDataEBeamV3" version="0">
            <dataset name="data">
                <attribute name="uDamageMask" method="damageMask"/>
                <attribute name="fEbeamCharge" method="ebeamCharge"/>
                <attribute name="fEbeamL3Energy" method="ebeamL3Energy"/>
                <attribute name="fEbeamLTUPosX" method="ebeamLTUPosX"/>
                <attribute name="fEbeamLTUPosY" method="ebeamLTUPosY"/>
                <attribute name="fEbeamLTUAngX" method="ebeamLTUAngX"/>
                <attribute name="fEbeamLTUAngY" method="ebeamLTUAngY"/>
                <attribute name="fEbeamPkCurrBC2" method="ebeamPkCurrBC2"/>
                <attribute name="fEbeamEnergyBC2" method="ebeamEnergyBC2"/>
                <attribute name="fEbeamPkCurrBC1" method="ebeamPkCurrBC1"/>
                <attribute name="fEbeamEnergyBC1" method="ebeamEnergyBC1"/>
            </dataset>
        </h5schema>
        <!-- ========================================================================= -->
        <h5schema name="BldDataPhaseCavity" version="0">
            <dataset name="data">
                <attribute name="fFitTime1" method="fitTime1"/>
                <attribute name="fFitTime2" method="fitTime2"/>
                <attribute name="fCharge1" method="charge1"/>
                <attribute name="fCharge2" method="charge2"/>
            </dataset>
        </h5schema>
        <!-- ========================================================================= -->
    
        <h5schema name="BldDataPimV1" version="0">

            <tag name="external" value="psddl_hdf2psana/bld.h"/>
            
        </h5schema>

        <!-- ========================================================================= -->
    
        <h5schema name="BldDataIpimbV0" version="0">

            <dataset name="data">
                <attribute name="ipimbData"/>
                <attribute name="ipimbConfig"/>
                <attribute name="ipmFexData"/>
            </dataset>
            
        </h5schema>

        <!-- ========================================================================= -->
    
        <h5schema name="BldDataIpimbV1" version="0">

            <dataset name="data">
                <attribute name="ipimbData"/>
                <attribute name="ipimbConfig"/>
                <attribute name="ipmFexData"/>
            </dataset>
            
        </h5schema>

        <!-- ========================================================================= -->

        <h5schema name="BldDataGMDV0" version="0">
            <dataset name="data">
                <attribute name="strGasType" method="gasType" rank="1"/>
                <attribute name="fPressure" method="pressure"/>
                <attribute name="fTemperature" method="temperature"/>
                <attribute name="fCurrent" method="current"/>
                <attribute name="fHvMeshElectron" method="hvMeshElectron"/>
                <attribute name="fHvMeshIon" method="hvMeshIon"/>
                <attribute name="fHvMultIon" method="hvMultIon"/>
                <attribute name="fChargeQ" method="chargeQ"/>
                <attribute name="fPhotonEnergy" method="photonEnergy"/>
                <attribute name="fMultPulseIntensity" method="multPulseIntensity"/>
                <attribute name="fKeithleyPulseIntensity" method="keithleyPulseIntensity"/>
                <attribute name="fPulseEnergy" method="pulseEnergy"/>
                <attribute name="fPulseEnergyFEE" method="pulseEnergyFEE"/>
                <attribute name="fTransmission" method="transmission"/>
                <attribute name="fTransmissionFEE" method="transmissionFEE"/>
            </dataset>
        </h5schema>

        <!-- ========================================================================= -->

        <h5schema name="BldDataGMDV1" version="0">
            <dataset name="data">
                <attribute name="fMilliJoulesPerPulse" method="milliJoulesPerPulse"/>
                <attribute name="fMilliJoulesAverage" method="milliJoulesAverage"/>
                <attribute name="fCorrectedSumPerPulse" method="correctedSumPerPulse"/>
                <attribute name="fBgValuePerSample" method="bgValuePerSample"/>
                <attribute name="fRelativeEnergyPerPulse" method="relativeEnergyPerPulse"/>
            </dataset>
        </h5schema>

    </package>

</psddl>
