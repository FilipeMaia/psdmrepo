<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE psddl SYSTEM "psddl.dtd">

<psddl>

    <use file="psddldata/epix.ddl.xml" />

    <package name="Epix">
    
        <!-- ========================================================================= -->
        
        <h5schema name="AsicConfigV1" version="0">
            <tag name="embedded"/>
            <tag name="default"/>
        </h5schema>

        <h5schema name="ElementV1" version="0">
            <dataset name="data">
                <attribute name="vc"/>
                <attribute name="lane"/>
                <attribute name="acqCount"/>
                <attribute name="frameNumber"/>
                <attribute name="ticks"/>
                <attribute name="fiducials"/>
                <attribute name="lastWord"/>
            </dataset>
            <dataset name="frame">
            </dataset>
            <dataset name="excludedRows">
                <tag name="zero_dims"/>
            </dataset>
            <dataset name="temperatures">
            </dataset>
        </h5schema>


    </package>

</psddl>
