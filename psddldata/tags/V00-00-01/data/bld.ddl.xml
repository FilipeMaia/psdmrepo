            <attribute name="fFitTime2" type="double" access="public">
            in pico-seconds
            </attribute>

            <attribute name="fCharge1" type="double" access="public">
            in pico-columbs
            </attribute>

            <attribute name="fCharge2" type="double" access="public">
            in pico-columbs
            </attribute>

		</pstype>
		

