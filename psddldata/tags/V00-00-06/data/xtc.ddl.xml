<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE psddl SYSTEM "psddl.dtd">

<psddl>
	<package name="Pds">

        <!-- ========================================================================= -->
	
		<pstype name="ClockTime">
		
		    <tag name="no-sizeof"/>
		    <tag name="value-type"/>
	        <tag name="external"/>
		
            <attribute name="_low" type="uint32_t" accessor="nanoseconds">
            </attribute>

            <attribute name="_high" type="uint32_t" accessor="seconds">
            </attribute>

		</pstype>
		
        <!-- ========================================================================= -->
	
		<pstype name="DetInfo">

            <tag name="no-sizeof"/>
		    <tag name="value-type"/>
	        <tag name="external"/>
		
            <attribute name="_low" type="uint32_t">
            </attribute>

            <attribute name="_phy" type="uint32_t">
            </attribute>

		</pstype>
		
		        
	</package>
</psddl>
